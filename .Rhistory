clean_lu_fact_matrices <- function(lu_frac_matrices_list=lu_frac_matrices_list,
cut_off_year){
lu_frac_matrix = lu_frac_matrices_list[[as.character(cut_off_year)]]
ind_unfilled_lu_class <- which(colSums(lu_frac_matrix)==0)
if(length(ind_unfilled_lu_class)>0){
name_unfilled_lu_class_w_year <-
colnames(lu_frac_matrix)[ind_unfilled_lu_class]
name_unfilled_lu_class <- sub('.*_', '', name_unfilled_lu_class_w_year)
cat("\nRemoving all entries for land use class:", name_unfilled_lu_class,
"due to a lack of observations in the reference year")
for(list_element in c(1:length(lu_frac_matrices_list))){
lu_frac_matrices_list[[list_element]] <- lu_frac_matrices_list[[list_element]][,-ind_unfilled_lu_class]
}
}
return(lu_frac_matrices_list)
}
